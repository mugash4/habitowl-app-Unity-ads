{
  "expo": {
    "name": "HabitOwl",
    "slug": "habitowl",
    "version": "2.9.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "habitowl",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.habitowl.app",
      "infoPlist": {
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "su67r6k2v3.skadnetwork"
          }
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSAdvertisingAttributionReportEndpoint": "https://postbacks-mobile.unity3d.com/v1/ios",
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.habitowl.app",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "com.google.android.gms.permission.AD_ID"
      ],
      "versionCode": 9,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#4f46e5"
    },
    "plugins": [
      "./plugins/withIronSource",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "minSdkVersion": 23,
            "enableProguardInReleaseBuilds": false,
            "usesCleartextTraffic": true
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#4f46e5"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.387609126713-1vpqhjkaha1ku86srq7lla63vqbpj98j"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ca88810f-4ffa-4120-a3f1-6364b7b5ea7f"
      }
    },
    "owner": "augustinemwathi6"
  }
}
